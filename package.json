{
  "name": "vertex-mcp-server",
  "version": "1.0.0",
  "description": "MCP server to proxy queries to Google Cloud Vertex AI (Gemini models)",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "vertex-mcp-server": "./build/index.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "watch": "tsc --watch",
    "dev": "tsx src/index.ts",
    "start": "node build/index.js",
    "clean": "rm -rf build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "vertex-ai",
    "gemini",
    "google-cloud"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/aiplatform": "^5.10.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
